<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="HOOQ" allow-return-values="true" verbose="2">
	<test name="Grouping" preserve-order="true">
		<parameter name="COUNTRY" value="IN" />
		<parameter name="ENV" value="prod" />
		<parameter name="ExType" value="Regression" />
		<parameter name="Module" value="IPhone" />
		<groups>
			<run>
				<exclude name="Visitor" />
			</run>
		</groups>
		<classes>
			<class name="com.automation.ios.iphone.scripts.HOOQ_IOS_IPhone_ME_Test"/>	
			<class name="com.automation.ios.iphone.scripts.HOOQ_IOS_IPhone_Search_Test"/>	
		</classes>
	</test>
</suite>